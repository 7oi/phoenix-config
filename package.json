{
	"name": "phoenix-config",
	"version": "1.0.0",
	"description": "Configuration for kasper/phoenix written in TypeScript",
	"main": "src/phoenix.ts",
	"dependencies": {
		"@types/phoenix": "github:mafredri/phoenix-typings#master",
		"rxjs": "^6.5.5",
		"typescript": "^3.9.3"
	},
	"devDependencies": {
		"prettier": "^2.0.5",
		"ts-loader": "^7.0.5",
		"tslint": "^6.1.2",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"scripts": {
		"start": "yarn run watch",
		"watch": "webpack --watch",
		"build": "webpack",
		"lint": "tslint src/**/*.ts",
		"format": "prettier --write \"src/**/*.ts\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mafredri/phoenix-config.git"
	},
	"keywords": [
		"phoenix",
		"typescript"
	],
	"author": "Mathias Fredriksson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mafredri/phoenix-config/issues"
	},
	"homepage": "https://github.com/mafredri/phoenix-config#readme",
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"singleQuote": true,
		"bracketSpacing": false,
		"trailingComma": "all"
	}
}
